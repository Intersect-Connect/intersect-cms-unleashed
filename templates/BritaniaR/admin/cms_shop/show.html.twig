{% block title %}CmsShop{% endblock %}

{% block body %}
    <h1>CmsShop</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cms_shop.id }}</td>
            </tr>
            <tr>
                <th>IdItem</th>
                <td>{{ cms_shop.idItem }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ cms_shop.price }}</td>
            </tr>
            <tr>
                <th>Forceddescription</th>
                <td>{{ cms_shop.forceddescription }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ cms_shop.quantity }}</td>
            </tr>
            <tr>
                <th>Promotion</th>
                <td>{{ cms_shop.promotion }}</td>
            </tr>
            <tr>
                <th>Visible</th>
                <td>{{ cms_shop.visible }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ cms_shop.name }}</td>
